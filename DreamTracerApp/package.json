{
  "name": "DreamTracerApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "build:android:release": "cd android && ./gradlew assembleRelease",
    "build:android:bundle": "cd android && ./gradlew bundleRelease",
    "clean": "react-native clean",
    "clean:android": "cd android && ./gradlew clean",
    "clean:metro": "npx react-native start --reset-cache",
    "optimize:images": "node scripts/optimize-images.js",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "build:analyze": "npm run build:android:bundle && npm run analyze:bundle",
    "optimize:all": "npm run optimize:images && npm run clean && npm run build:analyze",
    "bundle:android": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios/"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-firebase/app": "^23.3.0",
    "@react-native-firebase/auth": "^23.3.0",
    "@react-native/new-app-screen": "0.81.1",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@tanstack/react-query": "^5.86.0",
    "axios": "^1.11.0",
    "lottie-react-native": "^7.3.4",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-calendars": "^1.1313.0",
    "react-native-fs": "^2.20.0",
    "react-native-keychain": "^10.0.0",
    "react-native-nitro-modules": "^0.29.3",
    "react-native-permissions": "^5.4.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "^20.0.1",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "0.81.1",
    "@react-native/metro-config": "0.81.1",
    "@react-native/typescript-config": "0.81.1",
    "@tsconfig/react-native": "^3.0.6",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.12",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "sharp": "^0.33.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=20"
  }
}
